{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Content Published",
  "type": "object",
  "required": [
    "artifact_uri",
    "published_path"
  ],
  "properties": {
    "artifact_uri": {
      "type": "string",
      "pattern": "^s3://"
    },
    "published_path": {
      "type": "string"
    },
    "public_url": {
      "type": "string"
    },
    "jellyfin_item_id": {
      "type": "string"
    },
    "namespace": {
      "type": "string"
    },
    "meta": {
      "type": "object"
    }
  }
}