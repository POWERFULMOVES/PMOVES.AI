# Hi-RAG reranker providers
RERANK_ENABLE=true
RERANK_PROVIDER=flag           # flag|qwen|cohere|azure|tensorzero
RERANK_MODEL=Qwen/Qwen3-Reranker-4B
RERANK_MODEL_PATH=/models/qwen/Qwen3-Reranker-4B
RERANK_USE_FP16=true

# Qwen local (CrossEncoder)
QWEN_RERANK_MODEL=Qwen/Qwen3-Reranker-4B

# Cohere cloud
COHERE_API_KEY=
COHERE_RERANK_MODEL=rerank-english-v3.0

# Azure OpenAI cloud (JSON scoring)
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini
AZURE_OPENAI_API_VERSION=2024-06-01

# TensorZero function gateway
TENSORZERO_RERANK_FUNCTION=hi_rag_rerank
TENSORZERO_RERANK_TIMEOUT=20

RERANK_TOPN=50
RERANK_K=10
